datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String   @id   @map("telegramId")
  username   String?
  firstName  String?  @map("first_name")
  avatar     String?  @map("photo_url")
  createdAt  DateTime @default(now())
}
